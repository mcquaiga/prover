<UserControl x:Class="UnionGas.MASA.Dialogs.BarCodeDialog.BarCodeDialogView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             TextElement.FontSize="24"
             TextElement.FontWeight="Light"
             mc:Ignorable="d"
             d:DesignHeight="600" d:DesignWidth="800">
	<StackPanel Orientation="Vertical" Width="650" Height="200" Margin="10">
		<!--<TextBlock Margin="5">Company Number could not be found in an open job.</TextBlock>-->
		<TextBlock Margin="5" FontSize="32">Scan or enter device bar code</TextBlock>
		<TextBox Margin="10" FontSize="36" x:Name="BarCodeNumber" TabIndex="0" />
		<Grid>
			<Button x:Name="Cancel" HorizontalAlignment="Left" Margin="5">Skip</Button>
			<Button x:Name="Close" HorizontalAlignment="Right" Margin="5">OK</Button>
		</Grid>
	</StackPanel>
</UserControl>